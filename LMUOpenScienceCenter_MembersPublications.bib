@article{kwantMacrophagesLupusNephritis2022,
  title = {Macrophages in {{Lupus Nephritis}}: {{Exploring}} a Potential New Therapeutic Avenue},
  author = {Kwant, {\relax LE} and Vegting, Y and {Tsang-a-Sjoe}, {\relax MWP} and Kwakernaak, {\relax AJ} and Vogt, L and Voskuyl, {\relax AE} and {van Vollenhoven}, {\relax RF} and {de Winther}, {\relax MPJ} and Bemelman, {\relax FJ} and Anders, {\relax HJ} and Hilhorst, {\relax ML}},
  year = {2022},
