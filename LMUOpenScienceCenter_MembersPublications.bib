@article{caravaca-fontanManagementMembranousNephropathyan2022,
  title = {The Management of Membranous Nephropathy-an Update},
  author = {{Caravaca-Fontan}, F and {Fernandez-Juarez}, {\relax GM} and Floege, J and Goumenos, D and Kronbichler, A and Turkmen, K and {van Kooten}, C and Frangou, E and Stevens, {\relax KI} and Segelmark, M and Tesar, V and Anders, {\relax HJ} and Bruchfeld, A and {Immunonephrology Working Grp} and {European Renal Assoc}},
